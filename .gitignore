dockers/base_SXN/syslog-ng/cert/*.p12.b64
dockers/base_SXN/syslog-ng/cert/*.cer
dockers/base_SXN/syslog-ng/cert/*.crt
dockers/base_SXN/syslog-ng/cert/*.key

# SNMP traps persistent logs
dockers/base_SXN/zabbix-snmptraps/logs/

